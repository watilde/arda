  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "bluebird": "^2.9.6",
    "browserify": "^8.1.3",
    "cheerio": "^0.18.0",
    "coffee-script": "^1.9.0",
    "coffeeify": "^1.0.0",
    "espower-coffee": "^0.10.0",
    "jsdom": "^3.1.0",
    "mocha": "^2.1.0",
    "power-assert": "^0.10.1",
    "react": "0.13.0-beta.2",
    "rx": "^2.3.25",
    "sinon": "^1.12.2",
    "source-map-support": "^0.2.9"
  }
